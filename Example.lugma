struct Message {
  let id: UInt64
  let optionalField: String?
}

enum Error {
  case Gay
  case NoPermissions(
    needed: String
  )
}

flagset Permissions {
  flag CanRead
  flag CanChat
}

flagset Overrides: optional {
  flag CanRead
  flag CanChat
}

protocol Chat {
  func SendMessage(message: Message, message: Message) throws Error

  event MessageReceived(message: Message)
}
